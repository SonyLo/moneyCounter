


<!doctype html>
<html lang="ru">
<head>
    <!-- Required meta tags -->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>MoneyCounter</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
   
    

   
</head>
<body>


    <nav  class="navbar navbar-expand-lg navbar-dark nav-color">
        <a class="navbar-brand" href="#">MoneyCounter</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a class="nav-link" target="_blank" href="https://t.me/BotMoneyCosts_bot">Попробовать! <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#"></a>
                </li>

                <li class="nav-item">
                    <a class="nav-link " href="#" tabindex="-1" aria-disabled="true"></a>
                </li>
            </ul>

        </div>
    </nav>

    <div class="container-fluid ">
        @RenderBody()
        <hr />
        <footer class="text-center">
            <p>&copy; @DateTime.Now.Year - MoneyCounter</p>
        </footer>
    </div>




    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/particlesjs")
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
    @Scripts.Render("~/bundles/main")
    @RenderSection("scripts", required: false)

</body>
</html>

